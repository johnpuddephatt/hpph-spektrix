{
    "name": "hpph/template-fields",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "spatie/laravel-medialibrary-pro": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Hpph\\TemplateFields\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hpph\\TemplateFields\\FieldServiceProvider"
            ]
        }
    },
    "repositories": {
        "spatie": {
            "type": "composer",
            "url": "https://satis.spatie.be"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
