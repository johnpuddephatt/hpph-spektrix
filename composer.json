{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1.0",
        "advoor/nova-editor-js": "3.0.4",
        "armincms/many-to-many": "^1.0",
        "astrotomic/laravel-cachable-attributes": "^0.4.0",
        "blade-ui-kit/blade-icons": "^1.3",
        "cviebrock/eloquent-sluggable": "^9.0",
        "dillingham/nova-attach-many": "@dev",
        "doctrine/dbal": "^3.3",
        "ebess/advanced-nova-media-library": "4.0.3",
        "eminiarts/nova-tabs": "^2.1",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hpph/template-fields": "*",
        "itsgoingd/clockwork": "^5.1",
        "laravel/framework": "^9.0",
        "laravel/nova": "^4.0",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.10",
        "madewithlove/laravel-nova-uuid-support": "^1.4",
        "outl1ne/nova-menu-builder": "7.0.10",
        "outl1ne/nova-multiselect-field": "^4.0",
        "outl1ne/nova-settings": "^5.0",
        "outl1ne/nova-simple-repeatable": "^2.0",
        "outl1ne/nova-translations-loader": "^5.0",
        "spatie/laravel-activitylog": "^4.5",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-login-link": "^1.1",
        "spatie/laravel-medialibrary-pro": "^2.0.0",
        "spatie/laravel-tags": "^4.3",
        "spatie/nova-backup-tool": "^5.0",
        "spatie/nova-tags-field": "^4.0",
        "stepanenko3/nova-logs-tool": "^1.1",
        "whitecube/nova-flexible-content": "^1.0",
        "hpph/svg": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel-shift/blueprint": "^2.4",
        "laravel/pint": "^0.2.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": {
        "spatie": {
            "type": "composer",
            "url": "https://satis.spatie.be"
        },
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        "1": {
            "type": "path",
            "url": "./nova-components/TemplateFields"
        },
        "2": {
            "type": "path",
            "url": "./nova-components/nova-attach-many"
        },
        "3": {
            "type": "path",
            "url": "./nova-components/Svg"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}